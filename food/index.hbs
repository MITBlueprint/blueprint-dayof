<!DOCTYPE html>
<html>
<head lang="en">
  {{> shared_header context=headerContext}}

  {{> semantics_ui context=semanticsContext}}

  {{> meta_fb}}

  {{> meta_tw}}

  {{> ga context=gaContext }}

  {{> meta_viewport}}

  <script src="../assets/js/base.js"></script>
</head>

<body>

{{> pageDimmer }}


{{> nav context=navContext }}


{{> banner context=bannerContext }}
<div class="main width-limiter message-center">
  <div class="ui message">
    <p>
      Weâ€™ll be providing breakfast and lunch on Saturday and breakfast, lunch, and dinner on Sunday. Feel free to pick up your meal and take it with you to your table!
    </p>
  </div>
</div>

<div class="main">
  <div class="ui stackable two column page grid">
    {{#each events}}
    <div class="column">
      <div class="ui header">
        {{@key}}
      </div>
      <table class="ui table segment">
        <tbody>
          {{#each this}}
          <tr>
            <td>{{this.name}}</td>
            <td>{{this.time}}</td>
            <td>{{this.description}}</td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
    {{/each}}
  </div>
</div>

</body>
</html>
